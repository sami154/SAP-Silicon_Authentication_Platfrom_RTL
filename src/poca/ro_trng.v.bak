//wire [9:0] out2;
wire temp;
//XOR Tree
assign temp = out1[0]^out1[1]^out1[2]^out1[3]^out1[4]^out1[5]^out1[6]^out1[7];


always @(posedge clk)
begin
    if (rst == 1) begin
        cnt <= 0;
        done <= 0;
     end
    else if ( en == 1) begin
        cnt <= cnt+1;
            if (cnt == 200) begin
                cnt <= 0;
                done <= 1;
            end
     end
end

